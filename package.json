  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.2.1",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.12",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.6",
    "uuid": "^8.3.2"
  },
  "devDpendencies": {
    "nodemon": "^2.0.6",
    "concurrently": "^5.3.0"
  }
